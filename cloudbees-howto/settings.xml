<settings>
	<servers>
        <server>
            <id>ossrh</id>
            <username>[YOUR_USER_NAME]</username>
            <password>[YOUR_ENCRYPTED_PASSWORD]</password>
        </server>
	</servers>
 	<profiles>
    		<profile>
      			<id>ossrh</id>
      			<activation>
                    <activeByDefault>true</activeByDefault>
                </activation>
                <properties>
                    <gpg.useagent>true</gpg.useagent>
                    <gpg.executable>gpg2</gpg.executable>
                    <gpg.keyname>[KEY_HEX_CODE]</gpg.keyname>
                    <gpg.passphrase>[KEY_PASSPHRASE]</gpg.passphrase>
                    <gpg.defaultKeyring>false</gpg.defaultKeyring>
                    <gpg.lockMode>never</gpg.lockMode>
                    <gpg.homedir>/private/[YOUR_CLOUDBEES_USERNAME]/.gnupg</gpg.homedir>
                    <gpg.publicKeyring>/private/[YOUR_CLOUDBEES_USERNAME]/.gnupg/pubring.gpg</gpg.publicKeyring>
                    <gpg.secretKeyring>/private/[YOUR_CLOUDBEES_USERNAME]/.gnupg/secring.gpg</gpg.secretKeyring>
                </properties>
            </profile>
    </profiles>
</settings>
